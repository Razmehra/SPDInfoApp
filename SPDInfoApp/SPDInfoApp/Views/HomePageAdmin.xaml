<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SPDInfoApp.Views.HomePageAdmin">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Admin Homepage!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button x:Name="btnGetStudentData" Text="Get Student Data" HorizontalOptions="FillAndExpand" FontSize="12" Clicked="BtnGetStudentData_Clicked" />
            <Button x:Name="btnGetAlumniData" Text="Get Alumni Data" HorizontalOptions="FillAndExpand" FontSize="12" Clicked="BtnGetAlumniData_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>